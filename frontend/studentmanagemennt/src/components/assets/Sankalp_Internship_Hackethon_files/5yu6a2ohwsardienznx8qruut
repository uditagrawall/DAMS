(()=>{var e=document.querySelector(".exp-ac--toggle-click .ac__container"),t=e.querySelector(".ac__link.ac__link-semaphore"),a=document.getElementById("ads-container"),n=document.querySelector(".ac__container"),c=document.querySelector(".ta__header-text.ta__header-semaphore-text"),o=document.querySelector(".ta__header-text.ta__header-nonsemaphore-text"),r=document.querySelector(".ta__header"),i=document.getElementsByClassName("ta-creative"),s=document.getElementsByClassName("ta-creative__semaphore-selection"),l=t?t.dataset.contentType:a.dataset.contentType;function d(){a.classList.toggle("ads-container--semaphore-enabled"),c.classList.toggle("ta__header-text--inactive"),o.classList.toggle("ta__header-text--inactive"),n.classList.toggle("ac__container--inactive"),r.classList.toggle("ta__header--semaphore");for(var e=0;e<i.length;e+=1)i.item(e).classList.toggle("ta-creative--semaphore");for(var t=0;t<s.length;t+=1)s.item(t).classList.toggle("ta-creative__semaphore-selection--active")}function _(e,t,a,n){var c={type:"AD_MENU",contentType:e,creative:t,account:a,matchedAt:n};window.parent.postMessage(JSON.stringify(c),window.location.origin)}if(null!==e&&e.addEventListener("click",(function(){var e=t?t.dataset.contentType:a.dataset.contentType;"SAS_ADS_CREATIVE"===e?d():_(e,a.dataset.creative,a.dataset.account,a.dataset.matchedAt)})),null!==t)if("SAS_ADS_CREATIVE"===l){for(var u=function(){var e=s.item(m);e.addEventListener("click",(function(t){t.preventDefault(),_(l,e.dataset.creative,e.dataset.account,e.dataset.matchedAt),d()}))},m=0;m<s.length;m+=1)u();t.addEventListener("click",(function(e){e.preventDefault(),d()}))}else t.addEventListener("click",(function(e){e.preventDefault()}))})();